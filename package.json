{
  "name": "@pchmn/fly-admin",
  "version": "0.0.0-automated",
  "description": "A Typescript client for managing Fly infrastructure.",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "gen:types": "npx swagger-typescript-api -p https://docs.machines.dev/swagger/doc.json -o ./src/lib --extract-enums --extract-request-params --no-client -n types.ts",
    "all": "npm run build && npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pchmn/fly-admin.git"
  },
  "keywords": [
    "fly",
    "api",
    "client"
  ],
  "author": "pchmn",
  "license": "MIT",
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@typescript-eslint/parser": "^5.58.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.38.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
